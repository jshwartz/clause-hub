
  {{#if staticTrue}}
    {{block-static-form class="block-form" model=model destroyBlock=(action 'destroyStaticBlock') rebuildText=(action 'rebuildText') cancelEditing=(action (mut isEditing))}}
  {{else if dropdownTrue}}
    {{block-dropdown-form
      model=model
      rebuildDropdowns=rebuildDropdowns
      rebuildMenu=(action 'rebuildMenu')
      rebuildText=(action 'rebuildText')
      class="block-form"
      deleteDropdown=(action 'deleteDropdown')
      createNewDropdown=(action 'createNewDropdown')
      saveBlock=(action 'saveBlock')
      destroyBlock=(action 'destroyDropdownBlock')
      cancelEditing=(action (mut isEditing))}}
  {{else if toggleTrue}}
    {{block-toggle-form model=model rebuildMenu=(action 'rebuildMenu') destroyBlock=(action 'destroyStaticBlock') rebuildText=(action 'rebuildText') class="block-form" saveBlock=(action 'saveBlock') cancelEditing=(action (mut isEditing))}}
  {{else if checkboxTrue}}
    {{block-checkbox-form model=model rebuildMenu=(action 'rebuildMenu') destroyBlock=(action 'destroyCheckboxBlock') rebuildText=(action 'rebuildText') class="block-form"}}
  {{else if model.paragraph}}
    {{block-paragraph-form model=model rebuildMenu=(action 'rebuildMenu') destroyBlock=(action 'destroyStaticBlock') rebuildText=(action 'rebuildText') class="block-form"}}
  {{/if}}



{{outlet}}
